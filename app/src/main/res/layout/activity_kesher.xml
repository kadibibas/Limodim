<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/logo4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kadibibas.limodim.kesher"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/logo2"/>


    </android.support.v7.widget.Toolbar>


    <Button
        android:id="@+id/button2"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:background="@drawable/arrofi"
        android:layout_marginStart="4dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="4dp" />

    <Button
        android:id="@+id/kadi"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:background="@drawable/mailred"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton2"
        app:layout_constraintRight_toRightOf="@+id/imageButton2"
        android:layout_marginBottom="36dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton2" />

    <Button
        android:id="@+id/imageButton2"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:background="@drawable/tlf"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton3"
        app:layout_constraintRight_toRightOf="@+id/imageButton3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton3" />

    <Button
        android:id="@+id/imageButton3"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:background="@drawable/faceliu"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/googl"
        app:layout_constraintRight_toRightOf="@+id/googl"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="31dp"
        app:layout_constraintLeft_toLeftOf="@+id/googl" />

    <Button
        android:id="@+id/googl"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:background="@drawable/glogo"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="144dp"
        app:layout_constraintLeft_toLeftOf="parent" />




</android.support.constraint.ConstraintLayout>
